{
  "name": "tuncaybahadir/quar",
  "description": "A simple QR Code generation tool for your projects with Laravel 10, 11, 12 versions, php 8.3 and 8.4",
  "keywords": [
    "quar",
    "qr",
    "qrcode",
    "generator",
    "laravel",
    "laravel 10",
    "laravel 11",
    "laravel 12",
    "php",
    "php 8.3",
    "php 8.4"
  ],
  "homepage": "https://github.com/tuncaybahadir/quar",
  "license": "MIT",
  "authors": [
    {
      "name": "Tuncay BahadÄ±r",
      "email": "tuncaybahadir@protonmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "bacon/bacon-qr-code": "^3.0",
    "ext-gd": "*"
  },
  "require-dev": {
    "laravel/pint": "^1.24",
    "roave/security-advisories": "dev-latest"
  },
  "suggest": {
    "ext-imagick": "Allows the generation of PNG QrCodes.",
    "illuminate/support": "Allows for use within Laravel."
  },
  "autoload": {
    "psr-4": {
      "tbQuar\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "tbQuar\\QuarServiceProvider"
      ],
      "aliases": {
        "Quar": "tbQuar\\Facades\\Quar"
      }
    }
  },
  "scripts": {
    "project-optimize": [
      "composer validate",
      "./vendor/bin/pint"
    ]
  }
}
